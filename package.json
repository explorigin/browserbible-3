    "i18next": "^2.0.26",
    "i18next-browser-languagedetector": "0.0.14",
    "jquery": "^2.2.1",
    "jquery-i18next": "0.0.14",
    "jsdom": "=7.2.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "natural": "^0.2.1",
    "progress": "^1.1.8",
    "readline": "^1.3.0",
